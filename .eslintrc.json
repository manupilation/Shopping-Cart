{
  "extends": "trybe-fundamentals",
  "rules": {
    "no-alert": "off",
    "no-console": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "no-unused-vars": "warn",
    "no-param-reassign": "off",
    "max-lines-per-function": "off",
    "max-lines": "off",
    "no-use-before-define": "off"
  },
  "env": {
    "browser": true
  }
}
